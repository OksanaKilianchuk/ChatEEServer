<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>GetList</servlet-name>
        <servlet-class>GetListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetList</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>LogOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/logOut</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Room</servlet-name>
        <servlet-class>RoomServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Room</servlet-name>
        <url-pattern>/room</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetUsers</servlet-name>
        <servlet-class>GetAllUsersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUsers</servlet-name>
        <url-pattern>/getUsers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetStatus</servlet-name>
        <servlet-class>StatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetStatus</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>
</web-app>